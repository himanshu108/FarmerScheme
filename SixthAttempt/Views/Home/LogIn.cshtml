
@{
    ViewBag.Title = "LogIn";
}


@{
    ViewBag.Title = "Login";

}




<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>



    <div class="jumbotron text-center">
        <h2>Dot Net Training Case Study - Farm scheme </h2>

    </div>

    <div>

    </div>
    <h2 class="text-center">LogIn</h2>

    <div class="container">
        <div class="row">
            <div class="col-sm-4 center-block">
                <img src="https://i.pinimg.com/originals/3f/34/b2/3f34b2b917029cc9a0caddaa0a4454fb.png" class="img-thumbnail" width="278" height="278" />
                <a class="btn btn-success" href="https://localhost:44345/Admin/AdminProfile" role="button">Login As Admin</a>
            </div>
            <div class="col-sm-4 center-block">
                <img src="https://pngimg.com/uploads/farmer/farmer_PNG63.png" class="img-thumbnail" width="302" height="302" />
                <a class="btn btn-success" href="https://localhost:44305/Home/FarmerLogIn" role="button">Login As Farmer</a>



            </div>
            <div class="col-sm-4 center-block">
                <img src="https://cdn0.iconfinder.com/data/icons/auction-and-competition-outline/64/offer-offering-hand-give-bid-bidding-auction-512.png" class="img-thumbnail" width="278" height="278" />
                <a class="btn btn-success" href="https://localhost:44305/Home/BidderLogIn" role="button">Login As Bidder</a>
            </div>
        </div>
    </div>

    <div>

    </div>
    <div>
        <h2 class="text-center">Indian Gov Fasal Yojana</h2>
    </div>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="https://wallpapercave.com/wp/wp1886331.jpg" width="1200" height="500" alt="Yojana">
                <div class="carousel-caption">
                    <h3>Fasal Bima Yojana</h3>
                    <p>Indian Gov Fasal Bima Yojana!</p>
                </div>
            </div>

            <div class="item">
                <img src="https://i.pinimg.com/736x/d1/d5/11/d1d5112b2eaacab46f11e4d03e3103e2--agriculture-farmers.jpg" width="1200" height="500" alt="Yojana">
                <div class="carousel-caption">
                    <h3>Fasal Bima Yojana</h3>
                    <p>Indian Gov Fasal Bima Yojana!</p>
                </div>
            </div>

            <div class="item">
                <img src="https://thumbs.dreamstime.com/b/indian-farmer-holding-crop-plant-his-wheat-field-indian-farmer-holding-crop-plant-his-wheat-field-123557697.jpg" width="1200" height="500" alt="Yojana">
                <div class="carousel-caption">
                    <h3>Fasal Bima Yojana</h3>
                    <p>Indian Gov Fasal Bima Yojana!</p>
                </div>
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>


</body>
</html>




